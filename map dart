void main() {
  Map <String, dynamic> pessoas = {
    'nome': 'Feldhaus',
    'idade': 77,
    'altura': 190,
    'endereco': 'Quatro Pontes',
    'casado': true
  };
  String nome = pessoas['nome'];
  print(nome);
  int idade = pessoas['idade'];
  print(idade);
  int altura = pessoas['altura'];
  print(altura);
  String endereco = pessoas['endereco'];
  print(endereco);
  bool casado = pessoas['casado'];
  print(casado);

}


  
