void main() {
  
  String nome = "adrian";
  int idade = 17;
  
  if (idade <= 13) {
    print("$nome é uma criança");
  }
  else if (idade < 18) {
    print("$nome é adolecente meu fi");
  }
  else if (idade < 59) {
    print("$nome é adulto meu fi");
  }
 else if (idade > 60) {
   print("$nome é idoso");
 }
}

